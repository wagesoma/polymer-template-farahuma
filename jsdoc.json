{
    "source": {
      "include": ["contracts", "scripts"],
      "exclude": ["node_modules"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true
    },
    "plugins": ["plugins/markdown"]
  }
  